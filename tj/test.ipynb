   "source": [
    "from scipy.stats import moment, describe\n",
    "import numpy as np \n",
    "a = np.array([10, 20, 70])\n",
    "std = np.std(a) * 2\n",
    "print(a.mean(), std)\n",
    "result = a[np.where(np.logical_and(a>(a.mean() - std),a<(a.mean()+std)))]\n",
    "result\n"
   ]
